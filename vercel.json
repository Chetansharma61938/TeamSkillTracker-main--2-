{
    "version": 2,
    "buildCommand": "./vercel-build.sh",
    "outputDirectory": "dist",
    "framework": "vite",
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "functions": {
    "api/index.js": {
        "runtime": "@vercel/node@2.15.3",
        "memory": 1024,
        "maxDuration": 10
    }
    },
    "env": {
      "NODE_ENV": "production"
    }
  }
  